package mil.br.pamerj.projects.telefonia.dao;

/**
 *
 * @author fernandofot
 */
public abstract class DAOFactory {

    public abstract LigacaoDAO getLigacaoDAO();

    public abstract TipoLigacaoDAO getTipoLigacaoDAO();

    public abstract RamalDAO getRamalDAO();
    
    public abstract PessoaDAO getPessoaDAO();
    
    public abstract ConfigDAO getConfigDAO();
    
    public abstract FaturaDAO getFaturaDAO();
    
    public abstract AdminUserDAO getAdminUserDAO();
    
    public abstract FaturamentoDAO getFaturamentoDAO();
    
    
    
}
